{
	"name": "mathpix-markdown-vscode",
	"displayName": "Mathpix Markdown",
	"description": "Enable rendering Mathpix Markdown with latex and chemistry support.",
	"version": "0.0.1",
	"publisher": "mathpix",
	"repository": "https://github.com/mathpix/vscode-mathpix-markdown",
	"engines": {
		"vscode": "^1.32.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.helloWorld"
	],
	"main": "./dist/extension",
	"contributes": {
		"markdown.markdownItPlugins": true,
		"markdown.previewStyles": [
			"./mathpix-markdown.css"
		],
		"configuration": {
			"title": "Mathpix Markdown",
			"properties": {
				"mathpix-markdown.htmlTags": {
					"type": "boolean",
					"default": true,
					"description": "Enable htmlTags rendering."
				},
				"mathpix-markdown.latex": {
					"type": "boolean",
					"default": true,
					"description": "Enable LaTex rendering."
				},
				"mathpix-markdown.math": {
					"type": "boolean",
					"default": true,
					"description": "Enable math rendering with MathJax support."
				},
				"mathpix-markdown.chemistry": {
					"type": "boolean",
					"default": true,
					"description": "Enable chemistry rendering with SMILES support."
				},
				"mathpix-markdown-chemistry.disableColors": {
					"type": "boolean",
					"default": true,
					"description": "Disable all coloring."
				},
				"mathpix-markdown-chemistry.disableGradient": {
					"type": "boolean",
					"default": true,
					"description": "Disable gradient coloring."
				},
				"mathpix-markdown.dark_mode": {
					"type": "boolean",
					"default": false,
					"description": "Enable dark mode."
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "webpack --mode production",
		"webpack": "webpack --mode development",
		"webpack-dev": "webpack --mode development --watch",
		"test-compile": "tsc -p ./",
		"lint": "eslint . --ext .ts,.tsx"
	},
	"devDependencies": {
		"@types/node": "^12.12.0",
		"@types/vscode": "^1.32.0",
		"ts-loader": "^7.0.5",
		"@typescript-eslint/eslint-plugin": "^3.0.2",
		"@typescript-eslint/parser": "^3.0.2",
		"eslint": "^7.1.0",
		"typescript": "^4.0.2",
		"webpack": "^4.43.0",
		"webpack-cli": "^3.3.11"
	},
	"dependencies": {
		"mathpix-markdown-it": "^1.0.35"
	}
}
